#!/usr/bin/env sage

from Crypto.Util.number import long_to_bytes


A = 
U = 
V = 


u = lambda a, x, y: x + (a * x - y) / (x ^ 2 + y ^ 2)
v = lambda a, x, y: y - (x + a * y) / (x ^ 2 + y ^ 2)


P.<x> = PolynomialRing(ZZ)
f1 = (u(5, x, 4 * x) - A).numerator()
[a] = f1.roots(multiplicities=False)

P.<s, t> = PolynomialRing(ZZ)
f2 = (u(a, s, t) - U).numerator()
f3 = (v(a, s, t) - V).numerator()
f4 = f2.resultant(f3, t).polynomial(s).change_ring(ZZ)
[m1] = [i for i in f4.roots(multiplicities=False) if i > 0]
[m2] = f2.subs(s=m1).polynomial(t).change_ring(ZZ).roots(multiplicities=False)

print(long_to_bytes(m1) + long_to_bytes(m2))
